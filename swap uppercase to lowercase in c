#include<stdio.h> 
//#include 

void main() {

char ch;

//clrscr();

printf("Enter a String : ");

while( (ch=getchar( )) !='\n')  {
    
    if(ch>=65 && ch<=90) {
        putchar(ch+32);
     }
     else if(ch>=97 && ch<=122) {
    putchar(ch-32);
    }
    else {
        putchar(ch);
    }



}

//getch();  
   
}

